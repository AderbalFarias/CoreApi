{
  "ConnectionStrings": {
    "DemoConnection": "Data Source=localhost;Initial Catalog=dbDemo;User Id=sa;Password=afAdmin1;"
  },
  "AppSettings": {
    "ResponseErrorMessage": "Internal Server Error Test"
  },
  "CorsOrigin": [
    "http://localhost:4200",
    "https://localhost:5001"
  ],
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\CoreApp\\Api\\File.log",
          "fileSizeLimitBytes": "10000",
          "rollingInterval": "Day",
          "retainedFileCountLimit": "20",
          "rollOnFileSizeLimit": "true",
          "shared": "true"
        }
      }
    ]
  },
  "Authentication": {
    "Issuer": "http://localhost:51441/",
    "Audience": "CoreAppApiImplementation",
    "TokenEndpoint": "http://localhost:51441/connect/token"
  },
  "OidcAuthorizationServer": {
    "AccessTokenExpiration": "3600",
    "Audience": "CoreAppApiImplementation",
    "SigningCertificate": {
      "Subject": "localhost",
      "Store": "My",
      "Location": "LocalMachine",
      "AllowInvalid": "true"
    }
  }
}
