{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Connection": "Data Source=ServerTest;Initial Catalog=DBTest;User Id=test;Password=test;Trusted_Connection=False;MultipleActiveResultSets=true;"
  },
  "ConfigKeys": {
    "ResponseErrorMessage": "Internal Server Error Test"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Error",
        "Microsoft": "Error",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          //"serverUrl": "http://localhost:port",
          "path": "C:\\Logs\\CoreApi\\File.log",
          "fileSizeLimitBytes": "5000",
          "rollingInterval": "Day",
          "retainedFileCountLimit": "2",
          "rollOnFileSizeLimit": "true",
          "shared": "true"
        }
      }
    ]
  }
}
